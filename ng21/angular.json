{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm",
    "analytics": false,
    "schematicCollections": [
      "angular-eslint"
    ]
  },
  "newProjectRoot": "projects",
  "projects": {
    "demo1": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": true,
          "inlineStyle": true
        }
      },
      "root": "projects/demo1",
      "sourceRoot": "projects/demo1/src",
      "prefix": "fox",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/demo1/src/main.ts",
            "tsConfig": "projects/demo1/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/demo1/public"
              }
            ],
            "styles": [
              "projects/demo1/src/styles.css"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "demo1:build:production"
            },
            "development": {
              "buildTarget": "demo1:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular/build:unit-test"
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/demo1/**/*.ts",
              "projects/demo1/**/*.html"
            ],
            "eslintConfig": "projects/demo1/eslint.config.js"
          }
        },
        "e2e": {
          "builder": "playwright-ng-schematics:playwright",
          "options": {
            "devServerTarget": "demo1:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo1:serve:production"
            }
          }
        }
      }
    },
    "demo2": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": true,
          "inlineStyle": true
        }
      },
      "root": "projects/demo2",
      "sourceRoot": "projects/demo2/src",
      "prefix": "fox",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/demo2/src/main.ts",
            "tsConfig": "projects/demo2/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/demo2/public"
              }
            ],
            "styles": [
              "projects/demo2/src/styles.css"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "projects/demo2/src/environments/environment.ts",
                  "with": "projects/demo2/src/environments/environment.development.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "demo2:build:production"
            },
            "development": {
              "buildTarget": "demo2:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular/build:unit-test"
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/demo2/**/*.ts",
              "projects/demo2/**/*.html"
            ],
            "eslintConfig": "projects/demo2/eslint.config.js"
          }
        }
      }
    }
  }
}
